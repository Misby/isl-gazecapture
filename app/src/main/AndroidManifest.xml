    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.iai.mdf.Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.iai.mdf.Activities.SettingActivity" />
        <activity android:name="com.iai.mdf.Activities.DataCollectionActivity" />
        <activity android:name="com.iai.mdf.Activities.DataProcessActivity" />
        <activity android:name="com.iai.mdf.Activities.DemoRegrsActivity" />
        <activity android:name="com.iai.mdf.Activities.DemoClassActivity" />
        <activity android:name="com.iai.mdf.Activities.DemoiTrackerActivity" />
        <activity android:name="com.iai.mdf.Activities.DemoServerActivity2" />
        <activity android:name="com.iai.mdf.Activities.DemoServerActivity1" />
        <activity android:name="com.iai.mdf.Activities.TestPBActivity" />
        <activity android:name="com.iai.mdf.Activities.OpenCVActivity" />
        <activity android:name="com.iai.mdf.Activities.CloudActivity" />

    </application>

</manifest>